{
  "name": "ready",
  "version": "0.0.40",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "run:android": "npx expo run:android",
    "ios": "yarn amplify:pull:dev && npx expo run:ios --no-build-cache --no-install",
    "android": "yarn amplify:pull:dev && npx expo run:android --no-build-cache --no-install",
    "ios:staging": "yarn amplify:pull:staging && npx expo run:ios --no-build-cache --no-install",
    "ios:beta": "yarn amplify:pull:prod && npx expo run:ios --no-build-cache --no-install",
    "ios:prod": "yarn amplify:pull:prod && npx expo run:ios --no-build-cache --no-install",
    "prebuild:dev": "node bumpVersion.js dev patch",
    "prebuild:staging": "node bumpVersion.js staging patch",
    "prebuild:beta": "node bumpVersion.js beta patch",
    "prebuild:prod": "node bumpVersion.js prod patch",
    "update:env:dev": "node updateAmplifyEnv.js dev",
    "update:env:staging": "node updateAmplifyEnv.js staging",
    "update:env:beta": "node updateAmplifyEnv.js beta",
    "update:env:prod": "node updateAmplifyEnv.js prod",
    "amplify:pull:dev": "amplify pull --appId d2p9dqxkv4kz57 --envName dev && yarn update:env:dev",
    "amplify:pull:staging": "amplify pull --appId d2p9dqxkv4kz57 --envName staging && yarn update:env:staging",
    "amplify:pull:beta": "amplify pull --appId d2p9dqxkv4kz57 --envName beta && yarn update:env:beta",
    "amplify:pull:prod": "amplify pull --appId d2p9dqxkv4kz57 --envName prod && yarn update:env:prod",
    "ios:build:dev": "yarn amplify:pull:dev && eas build --profile development --platform ios --clear-cache --local",
    "ios:build:staging": "yarn amplify:pull:staging && eas build --profile staging --platform ios --clear-cache --local",
    "ios:build:beta": "yarn amplify:pull:beta && eas build --profile beta --platform ios --clear-cache --local",
    "ios:build:prod": "yarn amplify:pull:prod && eas build --profile prod --platform ios --clear-cache --local",
    "android:build": "eas build --profile preview --platform android --clear-cache --local",
    "ios:build:cloud:dev": "eas build --profile development --platform ios --clear-cache",
    "ios:build:cloud:staging": "eas build --profile staging --platform ios --clear-cache",
    "ios:build:cloud:beta": "eas build --profile beta --platform ios --clear-cache",
    "ios:build:cloud:prod": "eas build --profile prod --platform ios --clear-cache",
    "ios:prebuild": "npx expo prebuild --platform ios --clean",
    "android:prebuild": "npx expo prebuild --platform android --clean",
    "debug": "open 'rndebugger://set-debugger-loc?host=localhost&port=8081'",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "e2e:build-ios-debug": "detox build -c ios.sim.debug",
    "e2e:build-ios-release": "detox build -c ios.sim.release",
    "e2e:test-ios-debug": "detox test -c ios.sim.debug --loglevel verbose",
    "e2e:test-ios-release": "detox test -c ios.sim.release",
    "e2e:build-android-debug": "yarn android:clean && detox build -c android.emu.debug",
    "e2e:build-android-release": "yarn android:clean && detox build -c android.emu.release",
    "e2e:test-android-debug": "detox test -c android.emu.debug",
    "e2e:test-android-release": "detox test -c android.emu.release",
    "lint": "eslint ."
  },
