{
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react-native",
    "paths": {
      "@src/*": ["./src/*"],
      "@assets/*": ["./src/assets/*"],
      "@components/*": ["./src/components/*"],
      "@constants/*": ["./src/constants/*"],
      "@chatconfig/*": ["./src/chatConfig/*"],
      "@screens/*": ["./src/screens/*"],
      "@utils/*": ["./src/utils/*"],
      "@i18n/*": ["src/i18n/*"],
      "@locales/*": ["./src/locales/*"],
      "@store/*": ["./src/store/*"],
      "@api/*": ["./src/api/*"],
      "@graphql/*": ["./src/graphql/*"],
      "@gcq/*": ["./src/graphqlCustomQueries/*"],
      "@detox": ["./src/detox/*"]
    }
  },
  "extends": "expo/tsconfig.base"
}
